kw128: Q -> [4]L
kw128 K = kw
  where kw = [a,b,c,d]
        a = K@@[0..63]
        b = K@@[64..127]
        c = (KA(K,zero) <<< 111)@@[0..63]
        d = (KA(K,zero) <<< 111)@@[64..127]

