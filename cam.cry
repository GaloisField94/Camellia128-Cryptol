cam128e: (Q,Q) -> Q
cam128e (x,k) = y
  where y = join[yr,yl]
        yr = split`{2}(Round(y17,k128(k)@17))@1 ^ kw128(k)@2	 
        yl = split`{2}(Round(y17,k128(k)@17))@0 ^ kw128(k)@3  
