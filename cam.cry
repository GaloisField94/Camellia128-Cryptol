        y14 = Round(y13,k128(k)@13)
        y13 = Round(y12,k128(k)@12)
        y12 = join[y12l,y12r]
        y12l = FL(split`{2}(Round(y11,k128(k)@11))@0,kl128(k)@2)
        y12r = FL_1(split`{2}(Round(y11,k128(k)@11))@1,kl128(k)@3)
